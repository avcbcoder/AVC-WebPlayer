{
    "short_name": "My Extension",
    "name": "My Extension",
    "version": "1.0",
    "manifest_version": 2,
    "background": {
        "scripts":["app/background/communication/index.js", "app/background/network/index.js"]
    },
    "browser_action": { "default_popup": "app/popup/popup.html" },
    "content_scripts": [{
        "matches": ["<all_urls>", "*://*/_/chrome/newtab*", "*://*/*"],
        "css": ["/static/css/app.css"],
        "js": ["/static/js/content.js"]
    }],
    "web_accessible_resources": [
        "img/music_gif.gif",
        "img/*",
        "app/*",
        "app/background.js",
        "background.js",
        "app/background/cmmunication/index.js",
        "app/background/network/index.js"
    ],
    "permissions": [
        "activeTab",
        "tabs",
        "http://*/*",
        "https://*/*",
        "https://www.azlyrics.com/*",
        "management",
        "storage",
        "<all_urls>"
    ],
    "content_security_policy": "script-src 'self' 'sha256-GgRxrVOKNdB4LrRsVPDSbzvfdV4UqglmviH9GoBJ5jk='; object-src 'self'"
}